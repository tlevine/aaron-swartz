import urllib, re

r_ = re.compile('comics/boll/(\d\d\d\d/\d\d/\d\d)/boll/.*?<dd class="dek">(.*?)</dd>', re.S)
i = 19
url = "http://dir.salon.com/topics/ruben_bolling/%s.html"

while i:
    t = urllib.urlopen(url % i).read()
    j = r_.findall(t)
    j.reverse()
    for x in j:
        print '<a href="http://www.salon.com/comics/boll/%s/boll/story.gif">%s</a><br />'%x
