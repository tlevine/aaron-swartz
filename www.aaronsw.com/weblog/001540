function OpenTrackback (c) {
    window.open(c,
                    'trackback',
                    'width=480,height=480,scrollbars=yes,status=yes');
}

var HOST = 'www.aaronsw.com';

// Copyright (c) 1996-1997 Athenia Associates.
// http://www.webreference.com/js/
